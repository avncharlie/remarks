{
    "name": "Four storyboards",
    "author": "reMarkable",
    "templateVersion": "1.0.0",
    "formatVersion": 1,
    "categories": ["Creative"],
    "orientation": "landscape",
    "supportedDevices": ["rm2", "rmPP"],
    "constants": [
        {"mobileMaxWidth": 1000},
        {"mobileBoxWidth": 570},
        {"mobileOffsetY": 120},

        {"boxWidth": "templateHeight > mobileMaxWidth ? 838 : mobileBoxWidth"},
        {"boxHeight": 418},
        {"offsetY": "templateHeight > mobileMaxWidth ? 58 : mobileOffsetY"},
        {"lineSpacing": 58},
        {"horizontalSpacing": 76},
        {"offsetX": "templateWidth / 2 - boxWidth * 2 / 2 - horizontalSpacing / 2"},
        {"lineOffsetY": "boxHeight + 64"},
        {"groupHeight": 1260}
    ],
    "items": [
        {
            "type": "group",
            "boundingBox": {
                "x": 0,
                "y": 0,
                "width": "templateWidth",
                "height": 1404
            },
            "repeat": {
                "rows": "down"
            },
            "children": [
                {
                    "type": "group",
                    "boundingBox": {
                        "x": "offsetX",
                        "y": "offsetY",
                        "width": "boxWidth + horizontalSpacing",
                        "height": 663
                    },
                    "repeat": {
                        "columns": 2,
                        "rows": 2
                    },
                    "children": [
                        {
                            "type": "path",
                            "data": [
                                "M", 0, 0,
                                "L", "boxWidth", 0,
                                "L", "boxWidth", "boxHeight",
                                "L", 0, "boxHeight",
                                "Z"
                            ]
                        },
                        {
                            "type": "group",
                            "boundingBox": {
                                "x": 0,
                                "y": 0,
                                "width": "boxWidth",
                                "height": "lineSpacing"
                            },
                            "repeat": {
                                "rows": 3
                            },
                            "children": [
                                {
                                    "type": "path",
                                    "data": [
                                        "M", 0, "lineOffsetY", "L", "boxWidth", "lineOffsetY"
                                    ]
                                }
                            ]
                        }
                    ]
                }
            ]
        }
    ]
}
