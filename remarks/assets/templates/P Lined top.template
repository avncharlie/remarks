{
    "name": "Lined top",
    "author": "reMarkable",
    "templateVersion": "1.0.0",
    "formatVersion": 1,
    "categories": ["Lines"],
    "orientation": "portrait",
    "constants": [
        {"mobileMaxWidth": 1000},
        {"magicOffsetY": 1024},
        {"offsetY": "templateWidth < mobileMaxWidth || templateHeight < mobileMaxWidth ? templateHeight / 2 : magicOffsetY"}
    ],
    "items": [
         {
            "type": "group",
            "boundingBox": {
                "x": "templateWidth / 2 - templateHeight / 2",
                "y": "offsetY",
                "width": "templateHeight",
                "height": 78.7
            },
            "repeat": {
                "rows": "up"
            },
            "children": [
                {
                    "type": "path",
                    "data": [
                        "M", 0, 0, "L", "parentWidth", 0
                    ]
                }
            ]
        }
    ]
}
